[{"/Users/tammytruong/Code/USCukraine/USC_Website/src/index.js":"1","/Users/tammytruong/Code/USCukraine/USC_Website/src/App.js":"2","/Users/tammytruong/Code/USCukraine/USC_Website/src/reportWebVitals.js":"3","/Users/tammytruong/Code/USCukraine/USC_Website/src/interestForm.js":"4"},{"size":548,"mtime":1649275389070,"results":"5","hashOfConfig":"6"},{"size":7724,"mtime":1650301561050,"results":"7","hashOfConfig":"6"},{"size":362,"mtime":1649275389070,"results":"8","hashOfConfig":"6"},{"size":5133,"mtime":1650301378194,"results":"9","hashOfConfig":"6"},{"filePath":"10","messages":"11","suppressedMessages":"12","errorCount":0,"fatalErrorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":"13"},"1a9gslx",{"filePath":"14","messages":"15","suppressedMessages":"16","errorCount":0,"fatalErrorCount":0,"warningCount":7,"fixableErrorCount":0,"fixableWarningCount":5,"source":"17"},{"filePath":"18","messages":"19","suppressedMessages":"20","errorCount":0,"fatalErrorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":"13"},{"filePath":"21","messages":"22","suppressedMessages":"23","errorCount":0,"fatalErrorCount":0,"warningCount":1,"fixableErrorCount":0,"fixableWarningCount":0,"source":"24","usedDeprecatedRules":"25"},"/Users/tammytruong/Code/USCukraine/USC_Website/src/index.js",[],[],["26","27"],"/Users/tammytruong/Code/USCukraine/USC_Website/src/App.js",["28","29","30","31","32","33","34"],[],"import logo from './usc-logo.png';\nimport logo_mobile from './usc-logo-mobile.png';\nimport './App.css';\nimport 'react-dropdown/style.css';\nimport InterestForm from './interestForm.js';\n\nfunction App() {\n  return (\n    <div className=\"App\">\n      <header className=\"App-header container-fluid\">      \n      <div class=\"logo-wrapper static-top pt-0 pb-0\">\n            <img src={logo_mobile} class=\"logo-mobile d-sm-none justify-content-right\"/>\n            <img src={logo} class=\"logo d-none d-sm-block justify-content-right\"/>     \n      </div>\n      </header>\n      <body class=\"container-fluid p-0 m-0\">\n        \n        {/* ------------ HEADER SECTION ------------ */}\n        <div class=\"heading-title pt-5 pb-3 bg-image d-flex align-items-center justify-content-center\">\n          <div class=\"container-fluid\">\n            <h3 class=\"d-flex pb-0 mb-0 container-fluid justify-content-center\"><b>USC Ukraine Assistance Hub</b></h3>\n            <p class=\"d-flex pt-0 mt-0 container-fluid justify-content-center\"><br/>\n              <p id=\"subhead\">If you are a faculty member, postdoc, graduate, or undergraduate student who has left or is \n              considering leaving Ukraine because of the war, USC may offer you a place to continue your research, work or studies. Funding, visa support, accommodation, \n              mentoring, and other resources are available.</p>\n            </p>\n          </div>\n        </div>\n\n\n        {/* ------------ INTRODUCTION SECTION CODE ------------ */}\n        <div class=\"content container col-xs-6 mt-5 col-lg-7\">\n\n          <p>Please see <b>general pathways</b> as well as <b>specific opportunities</b> for students and scholars listed \n          below. Even if you do not see an existing opening, please submit your information through the Interest Form below. We will strive to connect you with specific \n          faculty and programs at USC.\n\n          Over 350 USC-based faculty have expressed interest in supporting Ukrainian students and academics, and resources are continually being updated internally!<br/><br/>\n        \n          <b>For USC Faculty:</b> If you'd like to add your resources or collaboration interests, please use <a href=\"https://docs.google.com/forms/d/e/1FAIpQLScsXK-KFfQnGe8jyvtR-CmS8L5YMyZ7VFKplx59SuyymKmeJA/viewform\" target='_blank'>this form</a>.<br/><br/>\n        \n          <i>This is a joint effort of USC faculty members with support and oversight by the Office of \n          Strategic and Global Initiatives at USC. For additional information, you can contact our working group directly via <a href=\"mailto:ukraineassistance@usc.edu\">ukraineassistance@usc.edu</a>.</i>\n          </p>\n        </div>\n        \n          \n        {/* ------------ GENERAL PATHWAYS SECTION ------------ */}\n        <div class=\"content container justify-content-center col-xs-6 mt-5 col-lg-7\">\n          <h4 class=\"section-headers d-flex pb-0 mb-0 m-0 p-0 container-fluid \">\n            General Pathways for Interested Scholars and Students\n          </h4><br/>\n\n          <table>\n            <tr>\n              <td class='table-program-pic'><p>\n                <img src='https://lh3.googleusercontent.com/gZrEU4O47bhYHpxq4NIPuNE60pkGsdp_uSQP6I25iUW8Ab1fDJAozs_qxm7u2s_B972__w=s85' \n                alt='Undergraduate Program'></img></p>\n              </td>\n              <td>\n                <p className='table-program-text'>\n                  <b>Undergraduate Programs: </b>  \n                  To learn more about undergraduate education at USC, please click <a target=\"_blank\" href=\"https://admission.usc.edu/\">here</a>.<br/><br/>\n                </p>\n              </td>\n            </tr>\n\n            <tr>\n              <td class='table-program-pic'>\n                <p><img src='https://lh3.googleusercontent.com/2YsteQ-Qykby1nz5RmXaMhZGwp2eD7Fd1suQv0uVmQ5jJdaL_E-Iu1cqZu_QMTPb3OjRRA=s85' \n                alt='Undergraduate Program'></img></p>\n              </td>\n              <td>\n                <p className='table-program-text'>\n                  <b>Master's Programs:</b> We may be able to offer you to continue your studies at \n                  USC while the war lasts. Funding, visa support, accommodation, mentoring, and other resources are available. To learn more, please fill out \n                  the <b><u><a href=\"#interest-form-link\">interest form</a></u></b> here. You may review a list of all master's programs at USC <a target=\"_blank\" href=\"https://gradadm.usc.edu/our-programs/\">here</a>.<br/><br/>\n                </p>\n              </td>\n            </tr>\n\n            <tr>\n              <td class='table-program-pic'>\n                <p><img src='https://lh3.googleusercontent.com/gfgvli9RsbGjX3hGuZ8EJQgPJw3nFeWohte_GPcsF-H4_xq5X59Zwol2CYAMqJtQQQjx=s85' \n                alt='Undergraduate Program'></img></p>\n              </td>\n              <td>\n                <p className='table-program-text'>\n                  <b>Doctoral Programs:</b> We may be able to offer you to continue your studies and \n                  research at USC. Lab space and placement in specific research teams may be available. Funding, visa support, accommodation, mentoring, and other \n                  resources are available. To learn more, please fill out the <b><u><a href=\"#interest-form-link\">interest form</a></u></b> here. You may review a list of all doctoral programs at USC <a target=\"_blank\" href=\"https://gradadm.usc.edu/our-programs/\">here</a>.<br/><br/>\n                </p>\n              </td>\n            </tr>\n\n            <tr>\n              <td class='table-program-pic'>\n                <p><img src='https://lh3.googleusercontent.com/hKdbgcVNUOMm4WcEWikQvOtHlkESjufsQUEyQIsc38UyT-bre5E6-t3Y5aGPjC0QfEeHaO4=s85' \n                alt='Undergraduate Program'></img></p>\n              </td>\n              <td>\n                <p className='table-program-text'>\n                  <b>Postdoctoral and Visiting Faculty Opportunities:</b> We may be able to offer you to \n                  continue your work or research at USC. Research and teaching opportunities may be available. Funding, visa support, accommodation, mentoring, and \n                  other resources are available. To learn more, please fill out the <b><u><a href=\"#interest-form-link\">interest form</a></u></b> here. Please fill the form out even if you apply directly via <a target=\"_blank\" href=\"https://usccareers.usc.edu/\">USC Careers</a>.\n                </p>\n              </td>\n            </tr>\n          </table>\n          {/* ------------ INTEREST FORM SECTION ------------ */}\n          <h4 class=\"form-header mt-4\" id=\"interest-form-link\">Interest Form</h4>\n          <InterestForm></InterestForm>\n          {/* <p>Click <a href=\"#\">here</a> for Non-USC Opportunities Suitable for Ukrainian Scholars/Students</p> */}\n        </div>\n\n        {/* ------------ CURRENT OPENINGS SECTION ------------ */}\n        {/* <div class=\"content container justify-content-center col-xs-6 mt-5 col-lg-7\">\n          <h4 class=\"section-headers d-flex pb-0 mb-0 container-fluid justify-content-center\">\n            Current Openings Suitable for Ukrainian Scholars/Students at USC\n          </h4><br/>\n\n          <table>\n            <tr>\n              <tc>\n                <h3>Opportunity 1</h3>\n                <p>Type, School/Department</p>\n                <a>URL</a>\n              </tc>\n              <br/><br/>\n              <tc class=\"opening2\">\n                <h3>Opportunity 2</h3>\n                <p>Type, School/Department</p>\n                <a>URL</a>\n              </tc>\n            </tr>\n          </table>\n\n        </div> */}\n\n      </body>\n\n      <footer class=\"footer container-fluid static-top\">\n      </footer>\n    </div>\n  );\n}\nexport default App;\n","/Users/tammytruong/Code/USCukraine/USC_Website/src/reportWebVitals.js",[],[],"/Users/tammytruong/Code/USCukraine/USC_Website/src/interestForm.js",["35"],[],"import { render } from 'react-dom';\nimport React from 'react';\nimport { Button, Form, Container, Header } from 'semantic-ui-react';\nimport './interestForm.css';\nimport axios from 'axios';\n\nclass InterestForm extends React.Component {\n    constructor(props) {\n        super(props)\n\n        this.state = {\n            Name: '',\n            Email: '',\n            CurrentResidence: '',\n            CurrentInstitution: '',\n            Position: '',\n            Interest: '',\n            USCDepartment: '',\n            Message: ''\n        }\n    }\n\n    // SUBMIT HANDLERS\n    changeHandler = (e) => {\n        this.setState({[e.target.name] : e.target.value})\n    }\n    submitHandler = e => {\n        e.preventDefault();\n        console.log(this.state);\n\n        axios.post('https://sheet.best/api/sheets/a185bc92-03fa-4be4-a316-a8a46ddd9e6f', this.state)\n        .then(response => {\n            console.log(response);\n        })\n        \n        .catch((error) => {\n            if (error.response){\n                console.log(error.response);\n            }else if(error.request){\n                console.log(error.request)\n            }else if(error.message){\n                console.log(error.message)\n            }\n        })\n        this.setState ({\n            Name: '',\n            Email: '',\n            CurrentResidence: '',\n            CurrentInstitution: '',\n            Position: '',\n            Interest: '',\n            USCDepartment: '',\n            Message: ''\n    });\n    }\n\n    render() {\n        const { Name, Email, CurrentResidence, CurrentInstitution, Position, Interest, USCDepartment, Message} = this.state;\n        \n        return (\n            <Container fluid className=\"form-container\">\n            <Form className=\"interest-form\" onSubmit={this.submitHandler}>\n                <Form.Field className=\"form-field\">\n                    <label>Name</label><br />\n                    <input placeholder='Name' type=\"text\" name=\"Name\" value={Name} onChange={this.changeHandler} required />\n                </Form.Field>\n                <Form.Field className=\"form-field\">\n                    <label>E-Mail</label><br />\n                    <input placeholder='E-mail' type=\"text\" name=\"Email\" value={Email} onChange={this.changeHandler} required />\n                </Form.Field>\n                <Form.Field className=\"form-field\">\n                    <label>Current place/country of residence</label><br />\n                    <input placeholder='Residence' type=\"text\" name=\"CurrentResidence\" value={CurrentResidence} onChange={this.changeHandler} required />\n                </Form.Field>\n                <Form.Field className=\"form-field\">\n                    <label>Current or most recent affiliation (institution)</label><br />\n                    <input placeholder='Institution' type=\"text\" name=\"CurrentInstitution\" value={CurrentInstitution} onChange={this.changeHandler} required/>\n                </Form.Field>\n\n                <Form.Field className=\"form-field\">\n                    <label>Current position</label><br />\n                    <select value={Position} name=\"Position\" onChange={this.changeHandler} type=\"select\" class=\"position\" required>\n                        <option value=\"\">Select a Position</option>\n                        <option value=\"faculty\">Faculty</option>\n                        <option value=\"postDoc\">PostDoctorate</option>\n                        <option value=\"phD\">PhD Student</option>\n                        <option value=\"masters\">Masters Student</option>\n                        <option value=\"undergrad\">Undergraduate Student</option>\n                        <option value=\"other\">Other</option>\n                    </select>\n                </Form.Field>\n\n                <Form.Field className=\"form-field\">\n                    <label>Your primary area(s) of interest</label><br />\n                    <input placeholder='Interests' type=\"text\" name=\"Interest\" value={Interest} onChange={this.changeHandler} required />\n                </Form.Field>\n                <Form.Field className=\"form-field\">\n                    <label>Your primary choice of School/Department/Program at USC</label><br />\n                    <input placeholder='School/Department' type=\"text\" name=\"USCDepartment\" value={USCDepartment} onChange={this.changeHandler} required/>\n                </Form.Field>\n                <Form.Field className=\"form-field\">\n                    <label>Message</label><br />\n                    <textarea rows=\"3\" className=\"message-field\" \n                        placeholder='Please tell us a little about yourself and what kinds of opportunities you are looking for. If you answered \"None\" or \"Other\" for any of the questions above, please explain here.' \n                        type=\"textarea\" name=\"Message\" value={Message} onChange={this.changeHandler} required/>\n                </Form.Field>\n                \n                <Button className=\"submit-button\" type='submit' >Submit</Button>\n\n            </Form>\n            </Container>\n        )\n    }\n}\n\nrender(<InterestForm />, document.getElementById(\"root\"));\nexport default InterestForm; \n",["36","37"],{"ruleId":"38","replacedBy":"39"},{"ruleId":"40","replacedBy":"41"},{"ruleId":"42","severity":1,"message":"43","line":12,"column":13,"nodeType":"44","endLine":12,"endColumn":89},{"ruleId":"42","severity":1,"message":"43","line":13,"column":13,"nodeType":"44","endLine":13,"endColumn":83},{"ruleId":"45","severity":1,"message":"46","line":40,"column":110,"nodeType":"44","messageId":"47","endLine":40,"endColumn":236,"fix":"48"},{"ruleId":"45","severity":1,"message":"46","line":63,"column":84,"nodeType":"44","messageId":"47","endLine":63,"endColumn":137,"fix":"49"},{"ruleId":"45","severity":1,"message":"46","line":77,"column":145,"nodeType":"44","messageId":"47","endLine":77,"endColumn":209,"fix":"50"},{"ruleId":"45","severity":1,"message":"46","line":91,"column":201,"nodeType":"44","messageId":"47","endLine":91,"endColumn":265,"fix":"51"},{"ruleId":"45","severity":1,"message":"46","line":105,"column":209,"nodeType":"44","messageId":"47","endLine":105,"endColumn":263,"fix":"52"},{"ruleId":"53","severity":1,"message":"54","line":3,"column":35,"nodeType":"55",